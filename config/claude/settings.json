{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "env": {
    "EDITOR": "cursor --wait",
    "VISUAL": "cursor --wait"
  },
  "permissions": {
    "allow": [
      "Read",
      "Write",
      "Edit",
      "Bash(git *)",
      "Bash(npm *)",
      "Bash(yarn *)",
      "Bash(pnpm *)",
      "Bash(bun *)",
      "Bash(make *)",
      "Bash(cargo *)",
      "Bash(go *)",
      "Bash(python *)",
      "Bash(mise *)",
      "Bash(docker *)",
      "Bash(gh *)",
      "Bash(ls *)",
      "Bash(cat *)",
      "Bash(grep *)",
      "Bash(find *)",
      "Bash(jq *)",
      "WebFetch"
    ],
    "deny": []
  },
  "enabledPlugins": {
    "ralph-loop@claude-plugins-official": true
  },
  "skipDangerousModePermissionPrompt": true,
  "effortLevel": "medium"
}
